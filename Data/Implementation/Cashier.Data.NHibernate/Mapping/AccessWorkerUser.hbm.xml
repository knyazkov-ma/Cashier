<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Cashier.Entity.AccessWorkerUser, Cashier.Entity" table="AccessWorkerUser" lazy="false">
    <id name="Id" column="Id" unsaved-value="0">
      <generator class="sequence">
        <param name="sequence">SQ_GLOBAL</param>
      </generator>
    </id>

    <property name="Type" type="Cashier.Entity.TypeAccessWorkerUserEnum, Cashier.Entity" not-null="true"/>
    <many-to-one foreign-key="AccessWorkerUser_WorkerUser_FK"   name="User"       fetch="join"    column="UserId"       class="Cashier.Entity.WorkerUser, Cashier.Entity" not-null="true"/>
    <many-to-one foreign-key="AccessWorkerUser_ObjectType_FK"   name="ObjectType" fetch="join"    column="ObjectTypeId" class="Cashier.Entity.ObjectType, Cashier.Entity"/>
    <many-to-one foreign-key="AccessWorkerUser_Organization_FK" name="Organization" fetch="join"  column="OrganizationId" class="Cashier.Entity.Organization, Cashier.Entity"/>
    <many-to-one foreign-key="AccessWorkerUser_Object_FK"       name="Object"     fetch="join"    column="ObjectId"     class="Cashier.Entity.RequestObject, Cashier.Entity"/>
    <many-to-one foreign-key="AccessWorkerUser_Worker_FK"       name="Worker"     fetch="join"    column="WorkerId"     class="Cashier.Entity.Worker, Cashier.Entity"/>
    
    <property name="OrganizationAddress" length="1000"/>
    
  </class>
</hibernate-mapping>