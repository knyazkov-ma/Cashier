<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Cashier.Entity.Model, Cashier.Entity" 
         table="Model" lazy="false">
    <id name="Id" column="Id" unsaved-value="0">
      <generator class="sequence">
        <param name="sequence">SQ_GLOBAL</param>
      </generator>
    </id>
    <property name="Name" length="200" not-null="true"/>
    <many-to-one  foreign-key="Model_Manufacturer_FK" name="Manufacturer"  column="ManufacturerId" class="Cashier.Entity.Manufacturer, Cashier.Entity" not-null="true"/>

  </class>
</hibernate-mapping>